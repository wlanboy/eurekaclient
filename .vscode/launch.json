{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "webserver:app",
                "--reload",
                "--log-level", "debug"
            ],
            "jinja": true,
            "console": "integratedTerminal"
        }
    ]
}